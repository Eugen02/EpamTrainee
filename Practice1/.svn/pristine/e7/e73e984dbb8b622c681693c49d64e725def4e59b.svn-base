package ua.nure.chekhunov.practice1;

public class Part2 {

	public static void sumNumber(int x, int y) {
		System.out.println(x + y);
	}

	public static void main(String[] args) {
		if (args.length != 0) {
		int x = Integer.parseInt(args[0]);
		int y = Integer.parseInt(args[1]);
		Part2.sumNumber(x, y);
		}
	}
}
